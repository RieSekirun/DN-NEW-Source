<?xml version="1.0" encoding="utf-8"?>
<angeloid.dreamnarae.SlideMenu xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/slideHolder"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >

    <ScrollView
        android:id="@+id/slidelist"
        android:layout_width="250dp"
        android:layout_height="fill_parent"
        android:background="@drawable/background_holo_light" >

        <LinearLayout
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <View
                android:layout_width="fill_parent"
                android:layout_height="2sp"
                android:background="#CCCCCC" />

            <Button
                android:id="@+id/mainscreen"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#00000000"
                android:drawableLeft="@drawable/icon_home"
                android:drawablePadding="15dip"
                android:gravity="left|center_vertical"
                android:onClick="mainscreen"
                android:paddingBottom="5dp"
                android:paddingLeft="10dip"
                android:paddingTop="5dp"
                android:text="@string/main"
                android:textSize="20sp" >
            </Button>

            <View
                android:layout_width="fill_parent"
                android:layout_height="2sp"
                android:background="#CCCCCC" />

            <Button
                android:id="@+id/updatelog"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/icon_updatelog"
                android:drawablePadding="15dip"
                android:gravity="left|center_vertical"
                android:background="#00000000"
                android:onClick="updatelog"
                android:paddingBottom="5dp"
                android:paddingTop="5dp"
                android:paddingLeft="10dip"
                android:text="@string/update"
                android:textSize="20sp" >
            </Button>

            <View
                android:layout_width="fill_parent"
                android:layout_height="2sp"
                android:background="#CCCCCC" />

            <Button
                android:id="@+id/spica"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/icon_spica"
                android:drawablePadding="15dip"
                android:gravity="left|center_vertical"
                android:background="#00000000"
                android:onClick="spicascreen"
                android:paddingBottom="5dp"
                android:paddingLeft="10dip"
                android:paddingTop="5dp"
                android:text="@string/spica"
                android:textSize="20sp" >
            </Button>

            <View
                android:layout_width="fill_parent"
                android:layout_height="2sp"
                android:background="#CCCCCC" />

            <Button
                android:id="@+id/pure"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#00000000"
                android:drawableLeft="@drawable/icon_pure"
                android:drawablePadding="15dip"
                android:paddingLeft="10dip"
                android:gravity="left|center_vertical"
                android:onClick="purescreen"
                android:paddingBottom="5dp"
                android:paddingTop="5dp"
                android:text="@string/pure"
                android:textSize="20sp" >
            </Button>

           <View
                android:layout_width="fill_parent"
                android:layout_height="2sp"
                android:background="#CCCCCC" />

            <Button
                android:id="@+id/save"
                android:layout_width="wrap_content"
                android:drawableLeft="@drawable/icon_save"
                android:drawablePadding="15dip"
                android:gravity="left|center_vertical"
                android:layout_height="wrap_content"
                android:paddingLeft="10dip"
                android:background="#00000000"
                android:onClick="savescreen"
                android:paddingBottom="5dp"
                android:paddingTop="5dp"
                android:text="@string/save"
                android:textSize="20sp" >
            </Button>
            <View
                android:layout_width="fill_parent"
                android:layout_height="2sp"
                android:background="#CCCCCC" />

            <Button
                android:id="@+id/prev"
                android:layout_width="wrap_content"
                android:drawableLeft="@drawable/icon_prev"
                android:drawablePadding="15dip"
                android:gravity="left|center_vertical"
                android:paddingLeft="10dip"
                android:layout_height="wrap_content"
                android:background="#00000000"
                android:onClick="prevscreen"
                android:paddingBottom="5dp"
                android:paddingTop="5dp"
                android:text="@string/prev"
                android:textSize="20sp" >
            </Button>
            <View
                android:layout_width="fill_parent"
                android:layout_height="2sp"
                android:background="#CCCCCC" />

            <Button
                android:id="@+id/miracle"
                android:layout_width="wrap_content"
                android:drawableLeft="@drawable/icon_miracle"
                android:drawablePadding="15dip"
                android:gravity="left|center_vertical"
                android:layout_height="wrap_content"
                android:background="#00000000"
                android:paddingLeft="10dip"
                android:onClick="miraclescreen"
                android:paddingBottom="5dp"
                android:paddingTop="5dp"
                android:text="@string/miracle"
                android:textSize="20sp" >
            </Button>
            <View
                android:layout_width="fill_parent"
                android:layout_height="2sp"
                android:background="#CCCCCC" />

            <Button
                android:id="@+id/brand"
                android:layout_width="wrap_content"
                android:drawableLeft="@drawable/icon_brand"
                android:drawablePadding="15dip"
                android:gravity="left|center_vertical"
                android:layout_height="wrap_content"
                android:paddingLeft="10dip"
                android:background="#00000000"
                android:onClick="brandscreen"
                android:paddingBottom="5dp"
                android:paddingTop="5dp"
                android:text="@string/brand"
                android:textSize="20sp" >
            </Button>
            <View
                android:layout_width="fill_parent"
                android:layout_height="2sp"
                android:background="#CCCCCC" />

            <Button
                android:id="@+id/spisave"
                android:layout_width="wrap_content"
                android:drawableLeft="@drawable/icon_spisave"
                android:drawablePadding="15dip"
                android:gravity="left|center_vertical"
                android:layout_height="wrap_content"
                android:paddingLeft="10dip"
                android:background="#00000000"
                android:onClick="spisavescreen"
                android:paddingBottom="5dp"
                android:paddingTop="5dp"
                android:text="@string/spisave"
                android:textSize="20sp" >
            </Button>
            <View
                android:layout_width="fill_parent"
                android:layout_height="2sp"
                android:background="#CCCCCC" />

            <Button
                android:id="@+id/delete"
                android:drawableLeft="@drawable/icon_delete"
                android:drawablePadding="15dip"
                android:gravity="left|center_vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#00000000"
                android:paddingLeft="10dip"
                android:onClick="deletescreen"
                android:paddingBottom="5dp"
                android:paddingTop="5dp"
                android:text="@string/delete"
                android:textSize="20sp" >
            </Button>
            <View
                android:layout_width="fill_parent"
                android:layout_height="2sp"
                android:background="#CCCCCC" />

            <Button
                android:id="@+id/promoting"
                android:drawableLeft="@drawable/icon_promoting"
                android:drawablePadding="15dip"
                android:gravity="left|center_vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#00000000"
                android:onClick="promotingscreen"
                android:paddingBottom="5dp"
                android:paddingLeft="10dip"
                android:paddingTop="5dp"
                android:text="@string/promoting"
                android:textSize="20sp" >
            </Button>
             <View
                android:layout_width="fill_parent"
                android:layout_height="2sp"
                android:background="#CCCCCC" />

            <Button
                android:id="@+id/setting"
                android:drawableLeft="@drawable/icon_settings"
                android:drawablePadding="15dip"
                android:gravity="left|center_vertical"
                android:layout_width="wrap_content"
                android:paddingLeft="10dip"
                android:layout_height="wrap_content"
                android:background="#00000000"
                android:onClick="settingscreen"
                android:paddingBottom="5dp"
                android:paddingTop="5dp"
                android:text="@string/setting"
                android:textSize="20sp" >
            </Button>
            <View
                android:layout_width="fill_parent"
                android:layout_height="2sp"
                android:background="#CCCCCC" />

            <Button
                android:id="@+id/developerinfo"
                android:drawableLeft="@drawable/icon_developerinfo"
                android:drawablePadding="15dip"
                android:gravity="left|center_vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#00000000"
                android:paddingLeft="10dip"
                android:onClick="developerinfoscreen"
                android:paddingBottom="5dp"
                android:paddingTop="5dp"
                android:text="@string/developerinfo"
                android:textSize="20sp" >
            </Button>
            <View
                android:layout_width="fill_parent"
                android:layout_height="2sp"
                android:background="#CCCCCC" />

            <Button
                android:id="@+id/donate"
                android:drawableLeft="@drawable/icon_donate"
                android:drawablePadding="15dip"
                android:gravity="left|center_vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#00000000"
                android:paddingLeft="10dip"
                android:onClick="donatescreen"
                android:paddingBottom="5dp"
                android:paddingTop="5dp"
                android:text="@string/donate"
                android:textSize="20sp" >
            </Button>
            <View
                android:layout_width="fill_parent"
                android:layout_height="2sp"
                android:background="#CCCCCC" />
        </LinearLayout>
    </ScrollView>

    <LinearLayout
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/main_layout2"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="#ffffff"
        android:orientation="vertical"
        tools:ignore="Overdraw" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#ffd9fe" >

            <LinearLayout
                android:layout_width="32.0dip"
                android:layout_height="32.0dip"
                android:layout_marginBottom="8.0dip"
                android:layout_marginLeft="12.0dip"
                android:layout_marginTop="8.0dip"
                android:background="@drawable/ic_launcher"
                android:onClick="eastereggevent" />

            <TextView
                android:id="@+id/tabtextview"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5.0dip"
                android:layout_marginTop="13.0dip"
                android:text="@string/app_name"
                android:textColor="#000000"
                android:textSize="9pt" />
        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <ViewFlipper
                android:id="@+id/viewFlipper1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="80dp"
                tools:ignore="UselessParent" >

                <ImageView
                    android:id="@+id/imageView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/main1" />

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/main2" />

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/main3" />

                <ImageView
                    android:id="@+id/imageView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/main4" />

                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/main5" />

                <ImageView
                    android:id="@+id/imageView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/main6" />

                <ImageView
                    android:id="@+id/imageView7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/main7" />

                <ImageView
                    android:id="@+id/imageView8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/main8" />

                <ImageView
                    android:id="@+id/imageView9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/main9" />

                <ImageView
                    android:id="@+id/imageView10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/main10" />

                <ImageView
                    android:id="@+id/imageView11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/main11" />
            </ViewFlipper>
        </RelativeLayout>
    </LinearLayout>

</angeloid.dreamnarae.SlideMenu>